{"ast":null,"code":"import request from \"../utils/request\";\nimport { Avatar, Lock } from \"@element-plus/icons-vue\";\nexport default {\n  name: \"register\",\n\n  data() {\n    return {\n      form: {},\n      rules: {\n        username: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }],\n        confirm: [{\n          required: true,\n          message: '请确认密码',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n\n  setup() {\n    return {\n      Lock,\n      Avatar\n    };\n  },\n\n  methods: {\n    register() {\n      this.$ref['form'].validate(valid => {\n        if (valid) {\n          console.log('submit!');\n\n          if (this.form.password !== this.form.confirm) {\n            this.$message({\n              type: \"error\",\n              message: \"两次密码输入不一致\"\n            });\n            return;\n          }\n\n          request.post(\"/user/register\", this.form).then(res => {\n            console.log(res);\n\n            if (res.code === '0') {\n              this.$message({\n                type: \"success\",\n                message: \"注册成功\"\n              });\n              this.$router.push(\"/login\"); //登录成功后进行页面跳转，跳转至主页\n            }\n          });\n        } else {\n          console.log('error submit!');\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyBA,OAAOA,OAAP,MAAoB,kBAApB;AACA,SAAQC,MAAR,EAAgBC,IAAhB,QAA2B,yBAA3B;AACA,eAAe;EACbC,IAAI,EAAE,UADO;;EAEbC,IAAI,GAAE;IACJ,OAAQ;MACNC,IAAI,EAAC,EADC;MAENC,KAAK,EAAC;QACJC,QAAQ,EAAC,CAAC;UAACC,QAAQ,EAAE,IAAX;UAAiBC,OAAO,EAAE,QAA1B;UAAoCC,OAAO,EAAE;QAA7C,CAAD,CADL;QAEJC,QAAQ,EAAC,CAAC;UAACH,QAAQ,EAAE,IAAX;UAAiBC,OAAO,EAAE,OAA1B;UAAmCC,OAAO,EAAE;QAA5C,CAAD,CAFL;QAGJE,OAAO,EAAC,CAAC;UAACJ,QAAQ,EAAE,IAAX;UAAiBC,OAAO,EAAE,OAA1B;UAAmCC,OAAO,EAAE;QAA5C,CAAD;MAHJ;IAFA,CAAR;EAQD,CAXY;;EAYbG,KAAK,GA