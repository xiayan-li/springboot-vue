{"ast":null,"code":"// // @ is an alias to /src\n// import HelloWorld from '@/components/HelloWorld.vue'\nimport { method } from 'lodash';\nimport request from \"../utils/request\";\nexport default {\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      total: 10,\n      pageSize: 5,\n      tableData: []\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"/user\", {\n        pageNum: this.currentPage,\n        pageSize: this.pageSize,\n        search: this.search\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    handleEdit() {},\n\n    handleSizeChange() {},\n\n    handleCurrentChange() {},\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      request.post(\"/user\", this.form).then(res => {\n        console.log(res);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoFA;AACA;AAEA,SAASA,MAAT,QAAuB,QAAvB;AACA,OAAOC,OAAP,MAAoB,kBAApB;AAKA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAC,EADA;MAELC,aAAa,EAAE,KAFV;MAGLC,MAAM,EAAE,EAHH;MAILC,WAAW,EAAE,CAJR;MAKLC,KAAK,EAAE,EALF;MAMLC,QAAQ,EAAE,CANL;MAOLC,SAAS,EAAE;IAPN,CAAP;EASD,CAXY;;EAYbC,OAAO,GAAE;IACP,KAAKC,IAAL;EACD,CAdY;;EAebC,OAAO,EAAE;IACPD,IAAI,GAAE;MACJV,OAAO,CAACY,GAAR,CAAY,OAAZ,EAAqB;QACnBC,OAAO,EAAE,KAAKR,WADK;QAEnBE,QAAQ,EAAE,KAAKA,QAFI;QAGnBH,MAAM,EAAE,KAAKA;MAHM,CAArB,EAGwBU,IAHxB,CAG6BC,GAAE,IAAI;QAC7BC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,KAAKP,SAAL,GAAiBO,GAAG,CAACd,IAAJ,CAASiB,OAA1B;QACA,KAAKZ,KAAL,GAAaS,GAAG,CAACd,IAAJ,CAASK,KAAtB;MAA4B,CANlC;IAQD,CAVM;;IAWPa,UAAU,GAAE,CAEX,CAbM;;IAcPC,gBAAgB,GAAE,CAEjB,CAhBM;;IAiBPC,mBAAmB,GAAE,CAEpB,CAnBM;;IAoBPC,GAAG,GAAE;MACH,KAAKnB,aAAL,GAAqB,IAArB;MACA,KAAKD,IAAL,GAAY,EAAZ;IACD,CAvBM;;IAwBPqB,IAAI,GAAE;MACJvB,OAAO,CAACwB,IAAR,CAAa,OAAb,EAAsB,KAAKtB,IAA3B,EAAiCY,IAAjC,CAAsCC,GAAE,IAA